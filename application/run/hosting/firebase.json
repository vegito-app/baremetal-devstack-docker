{
  "hosting": {
    "target": "vegito-app-legal-site",
    "public": "public/legal-sites",
    "rewrites": [
      {
        "source": "/fr/**",
        "destination": "/legal-sites/fr/index.html"
      },
      {
        "source": "/en/**",
        "destination": "/legal-sites/en/index.html"
      },
      {
        "source": "/nl/**",
        "destination": "/legal-sites/nl/index.html"
      },
      {
        "source": "/us/**",
        "destination": "/legal-sites/us/index.html"
      }
    ]
  }
}
