{
  "name": "Go Dev Container",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "service": "dev",
  "runServices": [
    "dev"
  ],
  "initializeCommand": "make local-builder-image local-docker-compose-firebase-emulators-build-no-pull",
  "postCreateCommand": "${localWorkspaceFolder}/.devcontainer/post-create-cmd.sh",
  "workspaceFolder": "/workspaces/refactored-winner",
  "customizations": {
    "vscode": {
      "settings": {
        "editor.fontLigatures": false,
        "editor.formatOnSave": true,
        "editor.inlineSuggest.enabled": true,
        "explorer.confirmDelete": false,
        "genieai.enableConversationHistory": true,
        "genieai.openai.model": "gpt-4",
        "genieai.promptPrefix.addComments-enabled": false,
        "git.enabled": true,
        "go.toolsManagement.autoUpdate": true,
        "search.showLineNumbers": true,
        "terminal.integrated.profiles.linux": {
          "bash": {
            "path": "bash",
            "icon": "terminal-debian",
            "color": "terminal.ansiRed"
          }
        },
        "terminal.integrated.defaultProfile.linux": "bash",
        "window.commandCenter": false,
        "workbench.colorTheme": "Kimbie Dark",
        "workbench.iconTheme": "material-icon-theme",
        "workbench.layoutControl.enabled": false,
        "zenMode.centerLayout": false,
        "zenMode.hideLineNumbers": false,
        "diffEditor.ignoreTrimWhitespace": false,
        "[jsonc]": {
          "editor.defaultFormatter": "vscode.json-language-features"
        },
        "files.hotExit": "onExitAndWindowClose",
        "remote.portsAttributes": {
          "3000": {
            "label": "Application UI - Debug",
            "protocol": "http",
            "onAutoForward": "silent"
          },
          "8080": {
            "label": "Application Backend - Debug",
            "protocol": "http",
            "onAutoForward": "silent"
          },
          "4000": {
            "label": "Firebase Emulator UI",
            "protocol": "http",
            "onAutoForward": "silent"
          },
          "4400": {
            "label": "Firebase Emulator Hub",
            "onAutoForward": "silent"
          },
          "4500": {
            "label": "Firebase reserved 0",
            "onAutoForward": "silent"
          },
          "5001": {
            "label": "Firebase Functions",
            "onAutoForward": "silent"
          },
          "8090": {
            "label": "Firebase Firestore",
            "onAutoForward": "silent"
          },
          "9000": {
            "label": "Firebase Database",
            "onAutoForward": "silent"
          },
          "9099": {
            "label": "Firebase Authentication",
            "onAutoForward": "silent"
          },
          "9150": {
            "label": "Firebase Reserved 1",
            "onAutoForward": "silent"
          },
          "9199": {
            "label": "Firebase Storage",
            "onAutoForward": "silent"
          },
          "9299": {
            "label": "Firebase (triggers)",
            "onAutoForward": "silent"
          }
        }
      },
      "extensions": [
        "ChFlick.firecode",
        "dbaeumer.vscode-eslint",
        "diemasmichiels.emulate",
        "esbenp.prettier-vscode",
        "genieai.chatgpt-vscode",
        "golang.go",
        "hasanakg.firebase-snippets",
        "hashicorp.hcl",
        "huang-an-sheng.firebase-web-app-snippets",
        "jsayol.firebase-explorer",
        "mauve.terraform",
        "ms-azuretools.vscode-docker",
        "msjsdiag.vscode-react-native",
        "pjmiravalle.terraform-advanced-syntax-highlighting",
        "pkief.material-icon-theme",
        "redhat.vscode-yaml",
        "standard.vscode-standard",
        "toba.vsfire",
        "hashicorp.terraform"
      ]
    }
  }
}